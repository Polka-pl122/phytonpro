import pyautogui
import time
pyautogui.moveTo(1365,758)
pyautogui.click()
pyautogui.moveTo(762,620)
pyautogui.doubleClick()
pyautogui.moveTo(769,495)
pyautogui.click()
pyautogui.hotkey('ctrl','a')
pyautogui.hotkey('ctrl','c')
pyautogui.hotkey('alt','f4')
pyautogui.moveTo(240,535)
pyautogui.doubleClick()
pyautogui.hotkey('ctrl','v')
pyautogui.moveTo(68,82)
pyautogui.click()
pyautogui.moveTo(73,150)
time.sleep(1)
pyautogui.click()
