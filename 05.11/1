x = str(input("Ваш имя? "))
y = str(input("Ваша фамилия? "))
z = int(input("Ваш возраст "))
b = str("Вы не проходите.")
if ((x!= "Микродий" and y != "Пармезанов")and(z >= 17)):
    a = str("Проходите.")
    print(a)
    
if (z <= 17):
    print(b)
    print("Вам нет 18.")
    
if (x== "Микродий" and y == "Пармезанов"):
    print(b)
    print("Вы в чёрном списке.")
