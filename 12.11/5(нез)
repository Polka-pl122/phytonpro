x = int(input('Введите номер столбца первой клетки '))
y = int(input('Введите номер строки первой клетки '))
z = int(input('Введите номер столбца второй клетки '))
a = int(input('Введите номер строки второй клетки '))
if (((x==2,4,6,8)and(y==2,4,6,8))and((z==2,4,6,8)and(a==2,4,6,8))):
    print('yes')
else:
    print('no')

if (((x==1,3,5,7,9)and(y==1,3,5,7,9))and((z=2,4,6,8)and(a==2,4,6,8))):
    print('no')
