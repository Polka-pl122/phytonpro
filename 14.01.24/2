sp = ['один','два','три']

x = input('Введите команду ')

if(x=='добавить'):
    y = input('Что добавить? ')
    sp.append(y)
    print(sp)
        
if(x=='удалить по индексу'):
    z=int(input('Введите номер элемента '))
    del sp[z]
    print(sp)

if(x=='удалить по названию'):
    a=input('Введите название элемента ')
    sp.remove(a)
    print(sp)
        
if(x=='показать элемент по номеру'):
    b=int(input('Номер элемента '))
    print(sp[b])

if(x=='показать весь список'):
    print(sp)

if(x=='переписать элемент по номеру'):
    c = int(input('Номер элемента 1 '))
    d = str(input('Название элемента 2 '))
    sp[c]=d
    print(sp)

